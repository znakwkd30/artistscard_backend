{
    "name": "project_support",
    "version": "0.0.1",
    "description": "Simple Project Support",
    "scripts": {
        "start": "cross-env NODE_ENV=prod tsc && node build/src/index.js",
        "start:dev": "cross-env NODE_ENV=dev ts-node-dev src/index.ts",
        "build": "tsc",
        "lint": "eslint ./src/**/*.{ts,tsx,js,jsx}",
        "lint:fix": "eslint --fix ./src/**/*.{ts,tsx,js,jsx}",
        "prettier": "prettier --write --config ./.prettierrc ./src/**/*.{ts,tsx}"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "class-transformer": "^0.3.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "morgan": "^1.10.0",
        "mysql": "^2.14.1",
        "reflect-metadata": "^0.1.10",
        "ts-node-dev": "^1.0.0",
        "typeorm": "0.2.29",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.9",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/moment": "^2.13.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^8.0.29",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.27.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.0",
        "ts-node": "3.3.0",
        "typescript": "3.3.3333"
    }
}
